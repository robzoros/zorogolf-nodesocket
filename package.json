{
  "name": "zorogolf",
  "version": "1.0.0",
  "description": "Juego de mesa de golf online",
  "main": "./server/server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/robzoros/zorogolf-nodesocket"
  },
  "keywords": [
    "nodejs",
    "react",
    "socketsio"
  ],
  "author": "Roberto MÃ©ndez",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "start npm run build_dev && start nodemon server/server.js",
    "start:prod": "npm run build && node server/server.js",
    "build:client": "webpack",
    "build:server": "webpack --config webpack.server.config.js",
    "build:client_dev": "webpack -w",
    "build:server_dev": "webpack -w --config webpack.server.config.js",
    "build": "npm run build:client",
    "build_dev": "npm run build:client_dev"
  },
  "dependencies": {
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "jwt-simple": "^0.5.0",
    "mongoose": "^4.9.3",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "socket.io": "^1.7.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "jsonfile": "^2.4.0",
    "css-loader": "^0.27.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "if-env": "^1.0.0",
    "nodemon": "^1.11.0",
    "sass-loader": "^6.0.3",
    "webpack": "^2.2.1"
  }
}
